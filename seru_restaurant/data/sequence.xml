<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="seq_seru_restaurant" model="ir.sequence">
            <field name="name">Sequence Restaurant</field>
            <field name="code">seq.seru.restaurant</field>
            <field name="prefix">SR/%(year)s/%(month)s/%(day)s</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_restaurant_menu" model="ir.sequence">
            <field name="name">Sequence Restaurant</field>
            <field name="code">seq.restaurant.menu</field>
            <field name="prefix">MN/%(year)s/%(month)s/%(day)s</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>